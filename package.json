{
  "name": "gatsby-magazine-site",
  "version": "1.0.0",
  "description": "Joseph Crawford's personal blog built with Gatsby",
  "private": true,
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
